{
    "name": "auth-user-service",
    "version": "1.0.0",
    "main": "dist/server.js",
    "scripts": {
        "dev": "ts-node-dev --respawn --transpile-only --ignore-watch node_modules src/server.ts",
        "dev:watch": "ts-node-dev --respawn --transpile-only --poll --interval 1000 --ignore-watch node_modules --watch src src/server.ts",
        "dev:docker": "docker-compose -f docker-compose.yml up --build",
        "dev:docker:up": "docker-compose up",
        "dev:docker:down": "docker-compose down",
        "dev:docker:logs": "docker-compose logs -f auth-service",
        "build": "tsc",
        "start": "node dist/server.js",
        "start:prod": "npm run build && npm run start",
        "migrate:dev": "prisma migrate dev --name init",
        "migrate:deploy": "prisma migrate deploy",
        "migrate:prod": "prisma migrate deploy",
        "generate": "prisma generate",
        "studio": "prisma studio",
        "db:reset": "prisma migrate reset",
        "db:seed": "prisma db seed",
        "docker:build": "docker-compose build",
        "docker:rebuild": "docker-compose up --build",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
