        "ioredis": "^5.7.0",
        "jsonwebtoken": "^9.0.2",
        "pg": "^8.16.3",
        "prisma": "^6.16.2",
        "prom-client": "^15.1.3",
        "rate-limiter-flexible": "^7.3.1",
        "redis": "^5.8.2",
